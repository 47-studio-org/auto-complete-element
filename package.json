{
  "name": "@github/auto-complete-element",
  "version": "0.7.0",
  "description": "Auto-complete input values from server results",
  "repository": "github/auto-complete-element",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint src/ test/ && flow check",
    "prebuild": "npm run clean && npm run lint",
    "build": "rollup -c && cp src/index.js.flow dist/index.esm.js.flow && cp src/index.js.flow dist/index.umd.js.flow",
    "pretest": "npm run build",
    "test": "karma start test/karma.config.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "auto-complete"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "chai": "^4.2.0",
    "eslint": "^5.14.1",
    "eslint-plugin-github": "^1.9.0",
    "flow-bin": "^0.93.0",
    "karma": "^4.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^6.0.0",
    "rollup": "^1.2.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "dependencies": {
    "@github/combobox-nav": "^0.2.2"
  }
}
